{
    "port" : "80", //Default is 80 if not set
    "aws" : {
        "default-region": "eu-west-1", //Set the default region here
        "default-env": "stag", //Set the default env here
        "default-cluster": "your-default-cluster", //Set the default cluster here
        "credentials": {
            //AWS Environment name, E.g. Prod, Staging, Dev. If not provided default will be used
            "staging" : {
                "key" : "your-aws-staging-key",
                "secret" : "your-aws-staging-secret",
            },
            "prod" : {
                "key" : "your-aws-prod-key",
                "secret" : "your-aws-prod-secret"
            }
        }
    },
    "slack-app" : {
        "clientId": "your-client-id",
        "clientSecret": "your-client-secret"
    }
}